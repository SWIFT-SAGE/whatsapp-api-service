                            <div class="row g-4 mt-4">
                                <div class="col-md-6">
                                    <div class="card">
                                        <div class="card-header">
                                            <h5>Session Status Check</h5>
                                        </div>
                                        <div class="card-body">
                                            <div class="mb-3">
                                                <label class="form-label">Session ID</label>
                                                <input type="text" class="form-control" id="status-session-id" placeholder="Enter session ID">
                                            </div>
                                            <button class="btn btn-success" id="check-status">
