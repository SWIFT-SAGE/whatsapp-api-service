                                            <div class="col-12">
                                                <button type="submit" class="btn btn-primary me-2">
                                                    <i class="fas fa-paper-plane me-2"></i>Send Test Message
                                                </button>
                                                <button type="button" class="btn btn-outline-secondary" id="generate-curl">
                                                    <i class="fas fa-code me-2"></i>Generate cURL
                                                </button>
                                            </div>
                                        </div>
                                    </form>

                                    <div id="test-response" class="mt-4" style="display: none;">
                                        <h6>Response:</h6>
                                        <pre class="bg-light p-3 rounded"><code id="response-content"></code></pre>
                                    </div>

                                    <div id="curl-output" class="mt-4" style="display: none;">
